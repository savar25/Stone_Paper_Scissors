<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stone_paper_scissors">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_new"
        android:label="Stone,Paper,\nScissors"
        android:roundIcon="@mipmap/ic_new_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".score_page"></activity>
        <activity android:name=".main_game" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>